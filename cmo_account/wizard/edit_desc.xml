<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="view_edit_desc" model="ir.ui.view">
			<field name="name">view.edit.desc</field>
			<field name="model">edit.desc</field>
			<field name="arch" type="xml">
				<form string="Edit Description">
					<field name="name"/>
					<footer>
						<button name="save" type="object" string="Save" class="oe_highlight"/>
						or
						<button special="cancel" class="oe_link" string="Cancel"/>
					</footer>
				</form>
			</field>
		</record>
	</data>
</openerp>
