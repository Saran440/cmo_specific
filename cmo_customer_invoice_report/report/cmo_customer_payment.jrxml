				<textElement>
					<font pdfFontName="Garuda.ttf" pdfEncoding="Identity-H"/>
				</textElement>
				<textFieldExpression class="java.util.Date"><![CDATA[$F{date_payment}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="153" y="250" width="210" height="20"/>
				<textElement>
					<font pdfFontName="Garuda.ttf" pdfEncoding="Identity-H"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{partner_tax_id}]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement x="20" y="250" width="144" height="20"/>
				<textElement>
					<font pdfFontName="Garuda.ttf" pdfEncoding="Identity-H"/>
				</textElement>
				<text><![CDATA[Tax ID/เลขประจำตัวผู้เสียภาษี]]></text>
			</staticText>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement x="80" y="184" width="280" height="37" isPrintWhenDetailOverflows="true"/>
				<textElement>
					<font pdfFontName="Garuda.ttf" pdfEncoding="Identity-H"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[(new Scriptlet()).getAddress(
    $F{partner_street},
    $F{partner_street2},
    $F{partner_city},
    $F{partner_zip},
    null,
    null)]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement x="80" y="160" width="280" height="24" isPrintWhenDetailOverflows="true"/>
				<textElement>
					<font pdfFontName="Garuda.ttf" pdfEncoding="Identity-H"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{partner_name}]]></textFieldExpression>
			</textField>
		</band>
	</pageHeader>
	<columnHeader>
		<band splitType="Stretch"/>
	</columnHeader>
	<detail>
		<band height="20" splitType="Prevent">
			<textField pattern="#,##0.00;-#,##0.00" isBlankWhenNull="true">
				<reportElement x="400" y="0" width="155" height="20"/>
				<box rightPadding="2"/>
				<textElement textAlignment="Right">
					<font pdfFontName="Garuda.ttf" pdfEncoding="Identity-H"/>
				</textElement>
				<textFieldExpression class="java.lang.Number"><![CDATA[$V{amount_untaxed} != null ? ($F{invoice_type} == "out_invoice" ? $V{amount_untaxed} : (-1) * $V{amount_untaxed}) : 0]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" pattern="" isBlankWhenNull="true">
				<reportElement x="15" y="0" width="385" height="20"/>
				<box topPadding="3" leftPadding="5" bottomPadding="3" rightPadding="5"/>
				<textElement>
					<font pdfFontName="Garuda.ttf" pdfEncoding="Identity-H"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{others_note}]]></textFieldExpression>
			</textField>
		</band>
