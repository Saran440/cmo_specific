					<font pdfFontName="Garuda.ttf" pdfEncoding="Identity-H"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{number} != null && $F{number} != "" ? "Ref." + $F{number} : ""]]></textFieldExpression>
			</textField>
		</band>
	</background>
	<title>
